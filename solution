*/ 
LANGUAGE C++
keep avariable mx which stores the maximum till ith element
Iterate over the array and update
mx=max(mx, a[i]) 
*/

#include <iostream>
#include <limits.h>
using namespace std;

int main()
{
    int n;
    cin>> n;
    int a[n];
    int mx;
    mx= INT_MIN;
    for(int i=0;i<n; i++){
        cin>> a[i];
    }
for(int i=0;i<n;i++){
    mx=max(mx,a[i]);
    cout<<mx<<endl;
}
    return 0;
}
//end of solution :)
